<section>
  <div class="row pt-5">
    {{#each products}}

    <div class="col-md-3">
      <div class="card" style="width:;">
        <img class="card-img-top" src="{{this.image}}" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title">{{this.name}}</h5>
          <p class="card-text">{{this.price}}</p>
          <a href="#" class="btn btn-primary">Add to Cart</a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</section>